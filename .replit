hidden = [".git","node_modules","dist/.vite","build",".cache",".next",".turbo","out"]

# Tip: we run manually from the shell to avoid duplicate processes.
# If you want auto-run later, set: run = "npm run dev"

[env]
PORT = "5000"
SKIP_VITE = "1"
NODE_OPTIONS = "--max-old-space-size=256"

[[ports]]
localPort = 5000
externalPort = 80
